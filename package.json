  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-powered-hiring-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-powered-hiring-system/issues"
  },
  "homepage": "https://github.com/yourusername/ai-powered-hiring-system#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
